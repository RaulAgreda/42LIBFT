char	*ft_strrchr(const char *s, int c)
{
	char	*last;
	char	*str;

	str = (char *)s;
	last = 0;
	while (*str != 0)
	{
		if (*str == c)
			last = str;
		str++;
	}
	if (c == 0)
		return (str);
	return (last);
}

